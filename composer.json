        "platform": {
            "php": "7.4"
        }
    },
    "require": {
        "http-interop/response-sender": "^1.0",
        "league/plates": "^3.3",
        "middlewares/method-override": "^1.1",
        "middlewares/payload": "^2.1",
        "middlewares/shutdown": "^1.1",
        "middlewares/whoops": "^1.2",
        "nikic/fast-route": "^1.3",
        "nyholm/psr7": "^1.1",
        "nyholm/psr7-server": "^0.3.0",
        "predis/predis": "^1.1",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "quanta/container": "^0.3.0",
        "quanta/http-dispatcher": "^0.4.0",
        "quanta/http-routing": "^0.2.0",
        "quanta/validation": "^0.2.0",
        "symfony/console": "^4.2",
        "symfony/dotenv": "^4.3",
        "symfony/process": "^4.3"
